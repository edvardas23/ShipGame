    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\GameServer\GameServer.csproj" />
    <ProjectReference Include="..\PackageClass\PacketClass.csproj" />
  </ItemGroup>

</Project>
